public class Directorio
{
    private Hashtable<String, String> Contactos;
    
    public Directorio(){
        this.Contactos = new Hashtable <String, String>();
    }
    
    public boolean AddContacts (String Nombre, String telefono){ //retorna true si se logro agregar, si ya existe retorna false
        boolean res = false;
        if(!this.Contactos.containsKey(Nombre)){
            this.Contactos.put(Nombre, telefono);
            res = true;
        }
        return false;
    }
    
    public String BuscaCasilla(String Nombre){
        i = hash(Nombre)%
    }
    
    public String Buscar(String Nombre){
        String res = "";
        //falta
        return res;
    }
    
    public void ListarContactos()
    {
      Enumeration lista = this.Contactos.keys();
      Object Nombre;
      Object telefono;
      while(lista.hasMoreElements())
      {
        Nombre = lista.nextElement();
        telefono=this.Contactos.get(Nombre);
        System.out.println(Nombre+"--"+telefono);
      }
    }
}
