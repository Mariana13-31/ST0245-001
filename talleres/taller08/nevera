/**
 * Esta clase se encarga de definir los atributos de una nevera.
 * 
 * @author Mariana Quintero, Isabella PÃ©rez 
 * @version 1
 */


public class Nevera{
	private int codigo;
	private String marca;
	
	public Nevera(int codigo, String marca){
		this.codigo = codigo;
		this.marca = marca;
	}
	
	public int getCodigo(){
		return this.codigo;
	}
	public String getMarca(){
		return this.marca;
	}
	
	public String imprimirNevera(){
		return "("+this.codigo+", '"+this.marca+"')";
	}
}
